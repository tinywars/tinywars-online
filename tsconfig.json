{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "useDefineForClassFields": true,
    "sourceMap": true,
    "strict": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "noImplicitOverride": true
  }
}
