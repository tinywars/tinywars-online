{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": true
  },
  "prettier.tabWidth": 4,
  "editor.tabSize": 4,
  "typescript.preferences.importModuleSpecifier": "project-relative",
  "typescript.preferences.quoteStyle": "double",
  "[json]": {
    "editor.tabSize": 2
  }
}
